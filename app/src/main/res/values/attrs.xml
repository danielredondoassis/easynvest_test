<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="customFont" format="string">
        <enum name="din_eng_std" value="1" />
        <enum name="din_mittel_std" value="2" />
        <enum name="din_neuzeit_grotesk_cond" value="3" />
        <enum name="din_neuzeit_grotesk_light" value="4" />
        <enum name="din_pro_black" value="5" />
        <enum name="din_pro_bold" value="6" />
        <enum name="din_pro_light" value="7" />
        <enum name="din_pro_medium" value="8" />
        <enum name="din_pro_regular" value="9" />
    </attr>

    <declare-styleable name="FontTextView">
        <attr name="customFont" />
    </declare-styleable>

    <declare-styleable name="FontButton">
        <attr name="customFont" />
    </declare-styleable>

    <declare-styleable name="FontEditText">
        <attr name="customFont" />
    </declare-styleable>

    <declare-styleable name="FontInputTextLayout">
        <attr name="customFont" />
    </declare-styleable>

    <declare-styleable name="MaskedEditText">
        <attr name="mask" format="string" />
        <attr name="placeholder" format="string" />
        <attr name="is_phone" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CustomInputText">
        <attr name="title_text" format="string"/>
        <attr name="edit_text" format="string"/>
        <attr name="hint" format="string"/>
        <attr name="clickable" format="boolean"/>
        <attr name="android:inputType"/>
        <attr name="android:imeOptions"/>
        <attr name="android:nextFocusDown"/>
        <attr name="edit_mask" format="string" />
        <attr name="edit_placeholder" format="string" />
        <attr name="edit_is_phone" format="boolean" />
        <attr name="hide_error" format="boolean" />
        <attr name="edit_is_registration_number" format="boolean" />
        <attr name="edit_ellipsize" format="boolean"/>
        <attr name="alternate_layout" format="integer"/>
        <attr name="bg_style" format="reference"/>

    </declare-styleable>

</resources>